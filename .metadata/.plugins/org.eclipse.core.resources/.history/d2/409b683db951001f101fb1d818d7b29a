package Number;

import java.util.Scanner;

public class ReverseNumber {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("enter the number : ");
		int num = scanner.nextInt();
		
		for (int i = num; i >=0; i--) {
			System.out.println(i);
		}

	}
}
